pipeline {
    agent any

    stages {


        stage('preapare Artifact') {
            steps {
                sh '''
                 zip -r ../frontend.zip *
                 '''
            }
        }

        stage('Upload Artifactory') {

            steps {
                sh '''
                    curl -f -v -u admin:admin123 --upload-file frontend.zip http://172.31.3.255:8081/repository/frontend/frontend.zip
                '''
            }

        }
    }
}