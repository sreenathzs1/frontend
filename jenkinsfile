pipeline {
    agent any

    stages {


        stage('preapare Artifact') {
            steps {
                sh '''
                 zip -r ../frontend.zip *
                 '''
            }
        }

        stage('Upload Artifacts') {
            steps {
                sh '''
                  curl -f -v -u admin:admin123 --upload-file frontend.zip http://3.237.64.177:8081/repository/frontend/frontend.zip
                  '''
           }

        }
    }
}